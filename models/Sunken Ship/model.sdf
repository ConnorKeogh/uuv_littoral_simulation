<?xml version="1.0"?>
<sdf version="1.6">
  <model name="sunkenship">
    <static>true</static>
    <link name="base_link">
      <collision name="collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://SunkenShip/meshes/sunkenship.dae</uri>
            <scale>1.0 1.0 1.0</scale>
          </mesh>
        </geometry>
      </collision>
      <visual name= "SunkenShip_visual">
        <geometry>
          <mesh>
            <uri>model://SunkenShip/meshes/sunkenship.dae</uri>
            <submesh>
              <name>SunkenShip</name>
              <center>false</center>
            </submesh>
          </mesh>
        </geometry>
        <material>
          <diffuse>1.0 1.0 1.0</diffuse>
          <specular>1.0 1.0 1.0</specular>
          <pbr>
            <metal>
              <albedo_map>model://SunkenShip/materials/textures/SunkenShip_Albedo.png</albedo_map>
              <normal_map>model://SunkenShip/materials/textures/SunkenShip_Normal.png</normal_map>
              <metalness_map>model://SunkenShip/materials/textures/SunkenShip_Metalness.png</metalness_map>
              <roughness_map>model://SunkenShip/materials/textures/SunkenShip_Roughness.png</roughness_map>
            </metal>
          </pbr>
          <!-- fallback to script if no PBR support-->
          <script>
            <uri>model://SunkenShip/materials/scripts/</uri>
            <uri>model://SunkenShip/materials/textures/</uri>
            <name>UrbanTile/SunkenShip</name>
          </script>
        </material>
      </visual>
    </link>
  </model>
</sdf>